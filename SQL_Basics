drop database if exists college;

create database college1;
Use college1;

Create table emp(
id INT Primary key,
name varchar(50),
marks INT ,
grade varchar(1),
city varchar(20)
);

insert into emp
(id,name,marks,grade,city)
values
(101,"ali",555,'A',"pak"),
(103,"weli",555,'A',"pak"),
(104,"alisg",555,'A',"pak"),
(102,"lio",999,'B',"ind");

select * from emp
